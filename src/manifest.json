{
    "name": "PocketDictionary",
    "version": "1.0",
    "minimum_chrome_version": "35.0",
    "manifest_version": 2,
    "permissions": [
      "contextMenus"
    ],
    "background": {
      "scripts": [
        "background.js"
      ]
    },
    "content_scripts": [
      {
        "matches": ["https://*.google.com/*"],
        "js": ["contentscript.js"]
      }
    ]
  }